@import url("default.css");

@import url("ansi.css");

th {
	background-color: #FFFFFF;
}

div.seealso, div.note {
	background-color: #e3dbd1;
	border: 1px solid #c9beb1;
}

pre {
	border-color: #c9beb1 -moz-use-text-color;
}

dt:target, .highlighted {
	background-color: #ffc465;
}
